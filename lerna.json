{
  "lerna": "3.16.4",
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "release*",
        "tiptap-image"
      ],
      "message": "[*] chore(release): %s"
    }
  },
  "packages": [
    "packages/!(template-*)/web"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "npmClientArgs": [
    "--no-lockfile"
  ],
  "version": "9.0.0"
}
